{
    "name": "league/shunt",
    "abandoned": true,
    "description": "PHP library for executing commands in parallel on multiple remote machines, via SSH",
    "keywords": ["ssh"],
    "homepage": "https://github.com/thephpleague/shunt",
    "license": "MIT",
    "authors": [
        {
            "name": "Taufan Aditya",
            "email": "toopay@taufanaditya.com"
        }
    ],
    "support": {
        "issues": "https://github.com/thephpleague/shunt/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "ext-ssh2": ">=0.12",
        "symfony/console": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.10",
        "symfony/finder": "~2.2",
        "symfony/process": "~2.1@dev",
        "league/phpunit-coverage-listener": "~1.0"
    },
    "replace": {
        "league/grunt": "*"
    },
    "autoload": {
        "psr-0": { "League\\Shunt": "src/" }
    },
    "bin": ["bin/shunt"]
}
